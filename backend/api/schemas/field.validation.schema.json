{
    "getSchema":{
       "type": "string",
       "required": [
         "id"     
       ],
       "properties": {
         "id" : {
           "type": "string"
         }
       }
    },
    "existSchema":{
      "type": "string",
       "required": [
         "name"     
       ],
       "properties": {
         "name" : {
           "type": "string"
         }
       }
    },
    "postSchema":{
        "type": "object",
        "required": [
          "name",
          "entity",
          "type"
        ],
        "properties": {
          "name" : {
            "type": "string",
            "minLength": 1,
            "maxLength": 50
          },
          "entity":{
            "type": "string",
            "enum": ["LEAD", "DEAL", "CONTACT","ACCOUNT"]
          },
          "type":{
            "type": "string",
            "enum":["TEXTBOX","DROPDOWN"]
          },
          "category":{
            "type":"string",
            "enum":["STRING", "NUMBER","MIXED"]
          }
        }
     }
}
